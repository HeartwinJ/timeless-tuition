    "@tabler/icons-vue": "^2.46.0",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "animejs": "^3.2.2",
    "dompurify": "^3.0.8",
    "marked": "^11.1.1",
    "trianglify": "^4.1.1",
    "vanilla-tilt": "^1.8.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue3-carousel": "^0.3.1"
